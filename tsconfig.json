{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": false,
    "removeComments": false,
    "sourceMap": false,
    "allowJs": true,
    "lib": ["webworker"]
  },
  "include": ["./**/*"]
} 